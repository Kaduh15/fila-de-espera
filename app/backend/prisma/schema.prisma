// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Admin {
  id       Int @id @default(autoincrement())
  password String

  @@map("admin")
}

model WaitingLine {
  id        String @id @default(uuid())
  customer String
  createdAt DateTime @default(now()) @map("created_at")
  initialServiceTime DateTime @db.Timestamp @map("initial_service_time")
  finishedServiceTime DateTime @db.Timestamp @map("finished_service_time")

  @@map("waiting_line")
}
